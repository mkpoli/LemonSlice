{
	"name": "lemon-slice",
	"main": ".svelte-kit/cloudflare/_worker.js",
	"compatibility_date": "2025-04-01",
	"compatibility_flags": ["nodejs_compat"],
	"assets": {
		"binding": "ASSETS",
		"directory": ".svelte-kit/cloudflare"
	},
	"observability": {
		"logs": {
			"enabled": true
		}
	},
	"vars": {
		"LEMON_AUTH_SERVER_ENDPOINT": "https://lemontv.win/login/jwt",
		"LEMON_JWT_PUBLIC_KEY_PATH": "https://lemontv.win/.well-known/jwks.json"
	}
}
